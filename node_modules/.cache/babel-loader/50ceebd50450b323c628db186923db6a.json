{"ast":null,"code":"// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\"; // Import Swiper styles\n\nimport 'swiper/scss';\nimport 'swiper/scss/navigation';\nimport 'swiper/scss/pagination'; // import required modules\n\nimport { Keyboard, Navigation, Pagination } from \"swiper\";\nexport default {\n  components: {\n    Swiper: Swiper,\n    SwiperSlide: SwiperSlide\n  },\n  setup: function setup() {\n    return {\n      pagination: {\n        clickable: true,\n        renderBullet: function renderBullet(index, className) {\n          return '<span class=\"' + className + '\">' + (index + 1) + \"</span>\";\n        }\n      },\n      modules: [Keyboard, Navigation, Pagination]\n    };\n  }\n};","map":{"version":3,"sources":["/Users/tsuchiyayuuto/Desktop/github/koganeisanpo/src/views/Pamphlet.vue"],"names":[],"mappings":"AAkDA;AACA,SAAS,MAAT,EAAiB,WAAjB,QAAoC,YAApC,C,CAEA;;AAEA,OAAO,aAAP;AACA,OAAO,wBAAP;AACA,OAAO,wBAAP,C,CAEA;;AACA,SAAS,QAAT,EAAmB,UAAnB,EAA+B,UAA/B,QAAiD,QAAjD;AAEA,eAAe;AACb,EAAA,UAAU,EAAE;AACV,IAAA,MAAM,EAAN,MADU;AAEV,IAAA,WAAW,EAAX;AAFU,GADC;AAKb,EAAA,KALa,mBAKL;AACN,WAAO;AACL,MAAA,UAAU,EAAE;AACV,QAAA,SAAS,EAAE,IADD;AAEV,QAAA,YAAY,EAAE,sBAAU,KAAV,EAAiB,SAAjB,EAA4B;AACxC,iBAAO,kBAAkB,SAAlB,GAA8B,IAA9B,IAAsC,KAAI,GAAI,CAA9C,IAAmD,SAA1D;AACD;AAJS,OADP;AAOL,MAAA,OAAO,EAAE,CAAC,QAAD,EAAW,UAAX,EAAsB,UAAtB;AAPJ,KAAP;AASF;AAfa,CAAf","sourcesContent":["<template>\n  <swiper\n    :slidesPerView=\"1\"\n    :centeredSlides=\"false\"\n    :slidesPerGroupSkip=\"1\"\n    :grabCursor=\"true\"\n    :keyboard=\"{\n      enabled: true,\n    }\"\n    :breakpoints=\"{\n      '769': {\n        slidesPerView: 2,\n        slidesPerGroup: 2,\n      },\n    }\"\n    :navigation=\"true\"\n    :pagination=\"pagination\" \n    :modules=\"modules\"\n    class=\"mySwiper\"\n  >\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-001.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-002.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-003.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-004.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-005.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-006.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-007.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-008.jpg\"\n    /></swiper-slide>\n    <swiper-slide\n      ><img src=\"https://cdn.magloft.com/github/swiper/images/page-009.jpg\"\n    /></swiper-slide>\n  </swiper>\n</template>\n<script>\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\n\n// Import Swiper styles\n\nimport 'swiper/scss';\nimport 'swiper/scss/navigation';\nimport 'swiper/scss/pagination';\n\n// import required modules\nimport { Keyboard, Navigation, Pagination } from \"swiper\";\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  setup() {\n    return {\n      pagination: {\n        clickable: true,\n        renderBullet: function (index, className) {\n          return '<span class=\"' + className + '\">' + (index + 1) + \"</span>\";\n        },\n      },\n      modules: [Keyboard, Navigation,Pagination],\n    };\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n#app { height: 100% }\nhtml,\nbody {\n  position: relative;\n  height: 100%;\n}\n\nbody {\n  background: #eee;\n  font-family: Helvetica Neue, Helvetica, Arial, sans-serif;\n  font-size: 14px;\n  color: #fff;\n  margin: 0;\n  padding: 0;\n}\n\n.swiper {\n  width: 60%;\n  height: 60%;\n  margin:0px auto 0px;\n  padding:50px 30px 80px;\n  \n}\n\n.swiper-slide {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  position: relative;\n}\n\n.swiper-slide img {\n  display: block;\n  width: 100%;\n}\n.swiper ::v-deep .swiper-button-next{\n    --swiper-navigation-color: #fff;\n    position: absolute;\n    right: 0px;\n    width: 26px;\n    height: 26px;\n    border-radius: 13px;\n    background-color: #ea6227;\n    & :after{\n      background-size: 10px 10px;\n    }\n}\n.swiper ::v-deep .swiper-button-prev ::after{\n    --swiper-navigation-color: #ea6227;\n}\n.swiper ::v-deep .swiper-pagination{\n  position: absolute;\n  bottom: 25px;\n  text-align: center;\n  transition: 300ms opacity;\n  transform: translate3d(0, 0, 0);\n  z-index: 10;\n}\n.swiper ::v-deep .swiper-pagination-bullet {\n  width: 25px;\n  height: 25px;\n  line-height: 25px;\n  text-align: center;\n  font-size: 10px;\n  color: #ea6227;\n  border: solid 1px #ea6227;\n  opacity: 1;\n  background: #fff;\n}\n.swiper ::v-deep .swiper-pagination-bullet-active {\n  color: #fff;\n  background: #ea6227;\n}\n\n@media only screen and (min-width: 769px) {\n  .swiper-slide:first-child {\n    transition: transform 100ms;\n  }\n\n  .swiper-slide:first-child img {\n    transition: box-shadow 500ms;\n  }\n\n  .swiper-slide.swiper-slide-active:first-child {\n    transform: translateX(50%);\n    z-index: 2;\n  }\n\n  .swiper-slide:nth-child(2) {\n    transition: transform 100ms;\n  }\n\n  .swiper-slide.swiper-slide-next:nth-child(2) {\n    transform: translateX(55%);\n    z-index: 1;\n  }\n\n  .swiper[dir=\"rtl\"] .swiper-slide.swiper-slide-active:first-child {\n    transform: translateX(-50%);\n  }\n\n  .swiper[dir=\"rtl\"] .swiper-slide.swiper-slide-next:nth-child(2) {\n    transform: translateX(-55%);\n  }\n}\n</style>\n\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}