{"ast":null,"code":"// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\"; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/effect-fade\";\nimport \"swiper/css/navigation\";\nimport \"swiper/css/pagination\";\nimport \"./style.css\"; // import required modules\n\nimport { EffectFade, Navigation, Pagination } from \"swiper\";\nexport default {\n  components: {\n    Swiper: Swiper,\n    SwiperSlide: SwiperSlide\n  },\n  setup: function setup() {\n    return {\n      modules: [EffectFade, Navigation, Pagination]\n    };\n  }\n};","map":{"version":3,"sources":["/Users/tsuchiyayuuto/Desktop/github/koganei/src/views/Pamphlet.vue"],"names":[],"mappings":"AA0BA;AACA,SAAS,MAAT,EAAiB,WAAjB,QAAoC,YAApC,C,CAEA;;AACA,OAAO,YAAP;AAEA,OAAO,wBAAP;AACA,OAAO,uBAAP;AACA,OAAO,uBAAP;AAEA,OAAO,aAAP,C,CAEA;;AACA,SAAS,UAAT,EAAqB,UAArB,EAAiC,UAAjC,QAAmD,QAAnD;AAEA,eAAe;AACb,EAAA,UAAU,EAAE;AACV,IAAA,MAAM,EAAN,MADU;AAEV,IAAA,WAAW,EAAX;AAFU,GADC;AAKb,EAAA,KALa,mBAKL;AACN,WAAO;AACL,MAAA,OAAO,EAAE,CAAC,UAAD,EAAa,UAAb,EAAyB,UAAzB;AADJ,KAAP;AAGD;AATY,CAAf","sourcesContent":["<template>\n  <swiper\n    :spaceBetween=\"30\"\n    :effect=\"'fade'\"\n    :navigation=\"true\"\n    :pagination=\"{\n      clickable: true,\n    }\"\n    :modules=\"modules\"\n    class=\"mySwiper\"\n  >\n    <swiper-slide\n      ><img\n        src=\"https://swiperjs.com/demos/images/nature-1.jpg\" /></swiper-slide\n    ><swiper-slide\n      ><img\n        src=\"https://swiperjs.com/demos/images/nature-2.jpg\" /></swiper-slide\n    ><swiper-slide\n      ><img\n        src=\"https://swiperjs.com/demos/images/nature-3.jpg\" /></swiper-slide\n    ><swiper-slide\n      ><img src=\"https://swiperjs.com/demos/images/nature-4.jpg\"\n    /></swiper-slide>\n  </swiper>\n</template>\n<script>\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\n\n// Import Swiper styles\nimport \"swiper/css\";\n\nimport \"swiper/css/effect-fade\";\nimport \"swi