{"ast":null,"code":"// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\"; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/scss/effect-cards\";\nimport 'swiper/scss/navigation'; // import required modules\n\nimport { EffectCards, Navigation } from \"swiper\";\nexport default {\n  components: {\n    Swiper: Swiper,\n    SwiperSlide: SwiperSlide\n  },\n  setup: function setup() {\n    return {\n      modules: [EffectCards, Navigation]\n    };\n  }\n};","map":{"version":3,"sources":["/Users/tsuchiyayuuto/Desktop/github/koganeisanpo/src/components/About.vue"],"names":[],"mappings":"AAwBA;AACA,SAAS,MAAT,EAAiB,WAAjB,QAAoC,YAApC,C,CAEA;;AACA,OAAO,YAAP;AACA,OAAO,0BAAP;AACA,OAAO,wBAAP,C,CAEA;;AACA,SAAS,WAAT,EAAsB,UAAtB,QAAuC,QAAvC;AAEA,eAAe;AACb,EAAA,UAAU,EAAE;AACV,IAAA,MAAM,EAAN,MADU;AAEV,IAAA,WAAW,EAAX;AAFU,GADC;AAKb,EAAA,KALa,mBAKL;AACN,WAAO;AACL,MAAA,OAAO,EAAE,CAAC,WAAD,EAAc,UAAd;AADJ,KAAP;AAGD;AATY,CAAf","sourcesContent":["<template>\n<section class=\"about\">\n    <swiper\n    :effect=\"'cards'\"\n    :grabCursor=\"true\"\n    :modules=\"modules\"\n    :initialSlide=\"3\"\n    :navigation=\"true\"\n    class=\"mySwiper\"\n    >\n        <swiper-slide></swiper-slide>\n        <swiper-slide></swiper-slide>\n        <swiper-slide></swiper-slide>\n        <swiper-slide></swiper-slide>\n        <swiper-slide></swiper-slide>\n        <swiper-slide></swiper-slide>\n        <swiper-slide></swiper-slide>\n    </swiper>\n    <figure class=\"about-background\"><img src=\"../assets/topslide-background.png\" alt=\"\"></figure>\n    <figure class=\"about-bunner\"><img src=\"../assets/topslide-banner.png\" alt=\"\"></figure>\n    <p class=\"about-lead\">我々こがねいさんぽは小金井市の観光を”<span class=\"about-lead-red\">発展させる</span>”学生コミュニティです。<br>活動内容は小金井市内にある小さいお子様連れが楽しめるような自然豊かな公園などの<br>魅力的なスポットを巡る<span class=\"about-lead-red\">パンフレットとサイトの作成</span>です。今まで知られていなかった<br>市内のスポットを散歩してもらい、魅力を発見してもらうことが狙いです。 </p>\n</section>\n</template>\n<script>\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\n\n// Import Swiper styles\nimport \"swiper/css\";\nimport \"swiper/scss/effect-cards\";\nimport 'swiper/scss/navigation';\n\n// import required modules\nimport { EffectCards, Navigation} from \"swiper\";\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  setup() {\n    return {\n      modules: [EffectCards, Navigation],\n    };\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n\nhtml,\nbody {\n  position: relative;\n  height: 100%;\n}\n*::before,\n*::after {\n    box-sizing: inherit; /* 継承 */\n}\n#app {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.about{\n    margin: 50px 0;\n    box-sizing: border-box;\n}\n.about-background img{\n    width:600px;\n    height:auto;\n    position: absolute;\n    right: calc(50% - 300px);\n    top: 500px;\n    z-index: 9000;\n}\n.about-bunner img{\n    width:500px;\n    height:auto;\n    position: absolute;\n    right: calc(50% - 250px);\n    top: 570px;\n    z-index: 10000;\n}\n.about-lead{\n    box-sizing: border-box;\n    width:580px;\n    margin: 0 auto;\n    font-size: 12px;\n    padding-top: 100px;\n    line-height: 200%;\n    color:#000;\n    &::before{\n        content: '';\n        background-image: url(\"../assets/topslide-before.png\");\n        background-size: 100% auto;\n        background-repeat: no-repeat;\n        width: 200px;\n        height: 600px;\n        position: absolute;\n        top: 0px;\n        left: 0;\n    }\n}\n.about-lead-red{\n    color: #e87778;\n}\n.swiper {\n  width: 500px;\n  height: 500px;\n}\n\n.swiper-slide {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 250px;\n  font-size: 22px;\n  font-weight: bold;\n  color: #fff;\n}\n.swiper ::v-deep .swiper-button-next{\n    --swiper-navigation-color: #fff;\n    position: absolute;\n    right: -100px;\n    width: 30px;\n    height: 30px;\n    border-radius: 15px;\n    background-color: #ea6227;\n}\n.swiper ::v-deep .swiper-button-next:after{\n  font-size: 10px;\n  --swiper-navigation-size:10px;\n}\n.swiper ::v-deep .swiper-button-prev{\n    --swiper-navigation-color: #fff;\n    position: absolute;\n    left: -100px;\n    width: 30px;\n    height: 30px;\n    border-radius: 15px;\n    background-color: #ea6227;\n}\n.swiper ::v-deep .swiper-button-prev:after{\n  font-size: 10px;\n  --swiper-navigation-size:10px;\n}\n.swiper ::v-deep .swiper-pagination{\n  position: absolute;\n  bottom: 25px;\n  text-align: center;\n  transition: 300ms opacity;\n  transform: translate3d(0, 0, 0);\n  z-index: 10;\n}\n.swiper ::v-deep .swiper-pagination-bullet {\n  width: 25px;\n  height: 25px;\n  line-height: 25px;\n  text-align: center;\n  font-size: 10px;\n  color: #ea6227;\n  border: solid 1px #ea6227;\n  opacity: 1;\n  background: #fff;\n}\n.swiper ::v-deep .swiper-pagination-bullet-active {\n  color: #fff;\n  background: #ea6227;\n}\n.swiper-slide:nth-child(1n) {\n  background-color: rgb(206, 17, 17);\n  background-image: url(\"../assets/topslide-1.jpg\");\n  background-size: 100% 100%;\n}\n\n.swiper-slide:nth-child(2n) {\n  background-color: rgb(0, 140, 255);\n  background-image: url(\"../assets/topslide-2.jpg\");\n  background-size: 100% 100%;\n}\n\n.swiper-slide:nth-child(3n) {\n  background-color: rgb(10, 184, 111);\n  background-image: url(\"../assets/topslide-3.jpg\");\n  background-size: 100% 100%;\n}\n\n.swiper-slide:nth-child(4n) {\n  background-color: rgb(211, 122, 7);\n  background-image: url(\"../assets/topslide-4.jpg\");\n  background-size: 100% 100%;\n}\n\n.swiper-slide:nth-child(5n) {\n  background-color: rgb(118, 163, 12);\n  background-image: url(\"../assets/topslide-5.jpg\");\n  background-size: 100% 100%;\n}\n\n.swiper-slide:nth-child(6n) {\n  background-color: rgb(180, 10, 47);\n  background-image: url(\"../assets/topslide-6.jpg\");\n  background-size: 100% 100%;\n}\n\n.swiper-slide:nth-child(7n) {\n  background-color: rgb(35, 99, 19);\n  background-image: url(\"../assets/topslide-7.jpg\");\n  background-size: 100% 100%;\n}\n.swiper ::v-deep .swiper-3d {\n    background: rgba(0,0,0,.15);\n    border-radius: 120px;\n}\n.swiper ::v-deep .swiper-slide-shadow {\n    background: rgba(0,0,0,.15);\n    border-radius: 250px;\n}\n</style>"],"sourceRoot":""},"metadata":{},"sourceType":"module"}