{"ast":null,"code":"import { Swiper, SwiperSlide } from \"swiper/vue\"; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/pagination\"; // import required modules\n\nimport { Pagination } from \"swiper\";\nexport default {\n  components: {\n    Swiper: Swiper,\n    SwiperSlide: SwiperSlide\n  },\n  setup: function setup() {\n    return {\n      pagination: {\n        clickable: true,\n        renderBullet: function renderBullet(index, className) {\n          return '<span class=\"' + className + '\">' + (index + 1) + \"</span>\";\n        }\n      },\n      modules: [Pagination]\n    };\n  }\n};","map":{"version":3,"sources":["/Users/tsuchiyayuuto/Desktop/github/koganei/src/views/Pamphlet.vue"],"names":[],"mappings":"AAUA,SAAS,MAAT,EAAiB,WAAjB,QAAoC,YAApC,C,CAEA;;AACA,OAAO,YAAP;AAEA,OAAO,uBAAP,C,CAEA;;AACA,SAAS,UAAT,QAA2B,QAA3B;AAEA,eAAe;AACb,EAAA,UAAU,EAAE;AACV,IAAA,MAAM,EAAN,MADU;AAEV,IAAA,WAAW,EAAX;AAFU,GADC;AAKb,EAAA,KALa,mBAKL;AACN,WAAO;AACL,MAAA,UAAU,EAAE;AACV,QAAA,SAAS,EAAE,IADD;AAEV,QAAA,YAAY,EAAE,sBAAU,KAAV,EAAiB,SAAjB,EAA4B;AACxC,iBAAO,kBAAkB,SAAlB,GAA8B,IAA9B,IAAsC,KAAI,GAAI,CAA9C,IAAmD,SAA1D;AACD;AAJS,OADP;AAOL,MAAA,OAAO,EAAE,CAAC,UAAD;AAPJ,KAAP;AASD;AAfY,CAAf","sourcesContent":["<template>\n  <swiper :pagination=\"pagination\" :modules=\"modules\" class=\"mySwiper\">\n    <swiper-slide>Slide 1</swiper-slide><swiper-slide>Slide 2</swiper-slide\n    ><swiper-slide>Slide 3</swiper-slide><swiper-slide>Slide 4</swiper-slide\n    ><swiper-slide>Slide 5</swiper-slide><swiper-slide>Slide 6</swiper-slide\n    ><swiper-slide>Slide 7</swiper-slide><swiper-slide>Slide 8</swiper-slide\n    ><swiper-slide>Slide 9</swiper-slide>\n  </swiper>\n</template>\n<script>\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\n\n// Import Swiper styles\nimport \"swiper/css\";\n\nimport \"swiper/css/pagination\";\n\n// import required modules\nimport { Pagination } from \"swiper\";\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  setup() {\n    return {\n      pagination: {\n        clickable: true,\n        renderBullet: function (index, className) {\n          return '<span class=\"' + className + '\">' + (index + 1) + \"</span>\";\n        },\n      },\n      modules: [Pagination],\n    };\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.swiper {\n  width: 100%;\n  height: 100%;\n}\n\n.swiper-slide {\n  background-position: center;\n  background-size: cover;\n}\n\n.swiper-slide img {\n  display: block;\n  width: 100%;\n}\n\n.swiper-pagination-bullet {\n  width: 20px;\n  height: 20px;\n  text-align: center;\n  line-height: 20px;\n  font-size: 12px;\n  color: #000;\n  opacity: 1;\n  background: rgba(0, 0, 0, 0.2);\n}\n\n.swiper-pagination-bullet-active {\n  color: #fff;\n  background: #007aff;\n}\n</style>\n\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}